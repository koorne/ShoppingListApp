<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ShoppingListFinal.Views.SettingsPage"
             xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
             Title="{xct:Translate Settings}"
             BackgroundColor="{AppThemeBinding 
                                  Dark={StaticResource WindowsBackgrondColorDark}, 
                                  Light={StaticResource WindowsBackgroundColor}}">
    <ContentPage.Content>
        <StackLayout>
            <Label Style="{StaticResource BaseLabel}" Text="To be finished"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>